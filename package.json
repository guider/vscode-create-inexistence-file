{
    "name": "vscode-create-inexitence-file",
    "displayName": "Create Inexitence File",
    "description": "create inexitence file like webstorm by shortcut alt + enter",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.35.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.createInexitenceFile"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [{
            "command": "extension.createInexitenceFile",
            "title": "create Inexitence File",
            "mac": "alt+enter"
        }],
        "keybindings": [{
			"command": "create Inexitence File",
			"key":"alt + enter"
        }]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^3.3.1",
        "vscode": "^1.1.28",
        "tslint": "^5.12.1",
        "@types/node": "^10.12.21",
        "@types/mocha": "^2.2.42"
    }
}
